{
  "name": "e2e-app",
  "main": "expo/AppEntry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "postinstall": "rm -rf ./node_modules/@fingerprintjs/fingerprintjs-pro-react-native/node_modules/react",
    "build:ios": "detox build --configuration ios.sim.release",
    "test:ios": "detox test --configuration ios.sim.release",
    "test:ios:ci": "detox test --configuration ios.sim.release --cleanup --headless --record-logs all"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@fingerprintjs/fingerprintjs-pro-react-native": "../",
    "expo": "~52.0.37",
    "expo-asset": "~11.0.4",
    "expo-build-properties": "~0.13.2",
    "expo-linking": "~7.0.5",
    "expo-splash-screen": "~0.29.22",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.77.1",
    "react-native-launch-arguments": "^4.1.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@config-plugins/detox": "^9.0.0",
    "@expo/config-plugins": "^9.0.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "detox": "^20.34.3",
    "eas-cli": "^15.0.12",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.3.3"
  },
  "private": true
}
